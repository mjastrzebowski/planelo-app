    "awesome-typescript-loader": "0.15.9",
    "fullcalendar": "^2.6.1",
    "ionic-gulp-scripts-copy": "^2.0.0",
    "strip-sourcemap-loader": "0.0.1",
    "typescript": "~2.3.2"
  },
  "cordovaPlugins": [
    "cordova-plugin-whitelist",
