    "@angular/core": "4.1.2",
    "@angular/forms": "4.1.2",
    "@angular/http": "4.1.2",
    "@angular/platform-browser": "4.1.2",
    "@angular/platform-browser-dynamic": "4.1.2",
    "@angular/platform-server": "4.1.2",
    "@ionic-native/core": "3.6.1",
    "@ionic/storage": "2.0.1",
    "angularfire2": "^2.0.0-beta.6",
    "event-stream": "^3.3.4",
    "firebase": "^3.5.0",
    "immutable": "~3.8.1",
    "ionic-angular": "3.3.0",
